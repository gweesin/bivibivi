{
  "name": "bivibivi",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@10.4.1",
  "description": "Merge and cut video for submitting bilibili",
  "author": "gweesin<gweesin2000@gmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/gweesin",
  "homepage": "https://github.com/gweesin/bivibivi#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gweesin/bivibivi.git"
  },
  "bugs": "https://github.com/gweesin/bivibivi/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.mjs",
    "./cli": "./dist/cli.mjs"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && pnpm publish",
    "start": "esno src/cli.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "@pnpm/core-loggers": "^1000.1.2",
    "@pnpm/default-reporter": "^1001.3.1",
    "@pnpm/logger": "^1000.0.0",
    "chalk": "^5.4.1",
    "commander": "^13.1.0",
    "execa": "^9.5.2",
    "fast-glob": "^3.3.3",
    "fs-extra": "^11.3.0",
    "node-lame": "^1.3.2",
    "p-limit": "^6.2.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.2.1",
    "@antfu/ni": "^23.3.1",
    "@antfu/utils": "^9.1.0",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.13.4",
    "bumpp": "^10.0.3",
    "eslint": "^9.20.1",
    "esno": "^4.8.0",
    "lint-staged": "^15.4.3",
    "pnpm": "^10.4.1",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.7.3",
    "unbuild": "^2.0.0",
    "vite": "^6.1.0",
    "vitest": "^3.0.5"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
